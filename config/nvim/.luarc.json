{
    "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
    "runtime": {
        "version": "LuaJIT",
        "path": [
            "lua/?.lua",
            "lua/?/init.lua"
        ]
    },
    "workspace": {
        "library": [
            "$VIMRUNTIME/lua",
            "${3rd}/luv/library",
            "$HOME/.local/share/nvim/lazy/lazy.nvim/lua"
        ],
        "checkThirdParty": false
    },
    "telemetry": {
        "enable": false
    }
}
